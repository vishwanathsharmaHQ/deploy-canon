{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/nodes/suggest": {
      "maxDuration": 60
    },
    "api/threads/generate": {
      "maxDuration": 60
    }
  }
} 